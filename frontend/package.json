{
  "name": "yomimashou",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.12.3",
    "axios": "0.24.0",
    "dmak": "0.3.1",
    "fabric": "2.7.0",
    "kuromoji": "0.1.2",
    "kuroshiro": "1.1.2",
    "kuroshiro-analyzer-kuromoji": "1.1.0",
    "mdi-react": "5.6.0",
    "node-sass": "4.14.1",
    "normalize.css": "8.0.1",
    "raphael": "2.3.0",
    "react": "17.0.2",
    "react-app-rewired": "^1.5.2",
    "react-dom": "17.0.2",
    "react-image-crop": "9.0.4",
    "react-masonry-infinite": "1.2.2",
    "react-outside-click-handler": "1.3.0",
    "react-pell": "0.4.0",
    "react-redux": "7.1.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "5.0.1",
    "recharts": "1.8.5",
    "redux": "4.1.2",
    "redux-promise-middleware": "5.1.1",
    "talkify-tts": "2.8.0",
    "typescript": "4.5.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --coverage --setupFiles ./src/setupTests.js",
    "test:ci": "react-scripts test --testResultsProcessor ./node_modules/jest-junit --coverage --coverageDirectory=test/coverage --setupFiles ./src/setupTests.js",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.12",
    "@types/react": "17.0.37",
    "@types/react-dom": "17.0.11",
    "@types/react-redux": "7.1.20",
    "@types/react-router-dom": "5.3.2",
    "@types/recharts": "1.8.23",
    "@wojtekmaj/enzyme-adapter-react-17": "0.6.5",
    "deepfreeze": "2.0.0",
    "enzyme": "3.11.0",
    "jest-junit": "13.0.0",
    "react-test-renderer": "17.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "coverageReporters": [
      "cobertura",
      "lcov",
      "text"
    ]
  },
  "jest-junit": {
    "suiteName": "jest tests",
    "output": "test/junit.xml",
    "classNameTemplate": "{classname} - {title}",
    "titleTemplate": "{classname} - {title}",
    "ancestorSeparator": " > ",
    "usePathForSuiteName": "true"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ]
}
